<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
   		 id="MainRunner"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run class="com.idega.app.eplatform.headless.MainRunner"/>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.idega.app.eplatform.headless.MainRunner"
            name="main">
         <property
               name="appName"
               value="main">
         </property>
      </product>
   </extension>

</plugin>
